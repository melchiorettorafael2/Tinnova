package application;

import java.util.Scanner;

import javax.swing.JOptionPane;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int fat = 1;

		int num = Integer.parseInt(JOptionPane.showInputDialog("Informe um nÃºmero inteiro"));

		while(num>=1) {
			fat *= (num-1);
			num--;
			System.out.println(fat);
		}
		
		sc.close();

	}

}
