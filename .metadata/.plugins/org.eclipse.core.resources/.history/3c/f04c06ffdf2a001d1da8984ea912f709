package application;

import java.util.Scanner;

import javax.swing.JOptionPane;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int fat = 1;
		int num = sc.nextInt();
		int var = num;

		while (num > 1) {
			fat *= num;
			num--;
		}

		System.out.println(var+"!= "+fat);

		sc.close();

	}

}
