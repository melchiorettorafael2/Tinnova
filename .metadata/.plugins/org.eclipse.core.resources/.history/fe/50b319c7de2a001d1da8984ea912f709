package application;

import java.util.Scanner;

import javax.swing.JOptionPane;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		double fat = 69;
		double x = fat;

		int num = Integer.parseInt(JOptionPane.showInputDialog("Informe um nÃºmero inteiro"));

		while(x>1) {
			fat *= (x-1);
			x--;
			
		}
		
		System.out.println(fat);
		
		sc.close();

	}

}
